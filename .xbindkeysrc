"i3-input"
	Mod4+i

"xdotool key Ctrl+w"
	b:8

"terminator"
	Mod4+Return

"google-chrome-stable"
	Shift+Mod4+Return

"thunar"
	Mod4+Control+Return

"dmenu -i -p " " -sb '#d64937' < ~/.cache/dmenu_run | /bin/sh &"
	Mod4+d

"dmenu_run"
	Mod4+Shift+d

"echo -e $(zenity --password --title=pm-suspend) | sudo -S pm-suspend"
	Shift+Mod4+s

"amixer set PCM 5%-"
	Mod4+F11

"amixer set PCM 5%+"
	Mod4+F12

"amixer set Master 5%+"
	XF86AudioRaiseVolume

"amixer set Master 5%-"
	XF86AudioLowerVolume

"xdotool key --window `xdotool getactivewindow` --clearmodifiers Up Up"
	Mod4+Shift+i

"xdotool key --window `xdotool getactivewindow` --clearmodifiers Down Down"
	Mod4+Shift+n

