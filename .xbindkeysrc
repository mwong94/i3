"xdotool key Ctrl+w"
	b:8

"terminator"
	Mod4+Return

"google-chrome-unstable"
	Shift+Mod4+Return

"dmenu_run"
	Mod4+d

"dmenu_run"
	release+Mod4+space

"echo -e $(zenity --password --title=pm-suspend) | sudo -S pm-suspend"
	Shift+Mod4+s

"amixer set PCM 5%-"
	Mod4+F11

"amixer set PCM 5%+"
	Mod4+F12

"amixer set Master 5%+"
	XF86AudioRaiseVolume

"amixer set Master 5%-"
	XF86AudioLowerVolume

"xdotool key --window `xdotool getactivewindow` --clearmodifiers Up Up"
	Mod4+i

"xdotool key --window `xdotool getactivewindow` --clearmodifiers Down Down"
	Mod4+n

"cmus-remote -u"
	F5

"cmus-remote -r"
	F7

"cmus-remote -s"
	F6

"cmus-remote -n"
	F8
